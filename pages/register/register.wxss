/* pages/register/register.wxss */
.step-title {
  margin-bottom: 30rpx;
  font-size: 28rpx;
  line-height: 1;
}
.success-img {
  width: 120rpx;
  height: 120rpx;
  display: block;
  margin: auto;
}
.success-txt {
  text-align: center;
  margin: 40rpx 0 50rpx;
  font-size: 28rpx;
}
.btns {
  display: flex;
  justify-content: space-between;
}
.to-home,
.to-login {
  width: 280rpx;
  margin: 0;
}
.to-home::after,
.to-login::after {
  border: none;
}
.picker-region {
  flex: 1;
}